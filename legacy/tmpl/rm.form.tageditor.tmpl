<script id="tagEditorTemplate" type="text/x-jquery-tmpl">
<div class="rm_mbox_shader">
    <div class="rm_mbox_wrap dynTop">
        <div class="rm_mbox_header">
            Track Metadata Editor
            <div class="rm_mbox_close_btn">
                <img src="/images/closeButton.gif" />
            </div>
        </div>
        <div class="rm_mbox_text">
            <form>
                <table class="rm_tagedit_table">
                    <tr><td>Track ID</td><td><input name="track_id" class="rm_ui_input_text" type="text" id="tagID" value="${tid}" readonly /></td></tr>
                    <tr><td>Title</td><td><input name="title" class="rm_ui_input_text" type="text" id="tagTitle" value="${title}" /></td></tr>
                    <tr><td>Artist</td><td><input name="artist" class="rm_ui_input_text" type="text" id="tagArtist" value="${artist}" /></td></tr>
                    <tr><td>Album</td><td><input name="album" class="rm_ui_input_text" type="text" id="tagAlbum" value="${album}" /></td></tr>
                    <tr><td>Date</td><td><input name="date" class="rm_ui_input_text" type="text" id="tagDate" value="${date}" /></td></tr>
                    <tr><td>Genre</td><td><input name="genre" class="rm_ui_input_text" type="text" id="tagGenre" value="${genre}" /></td></tr>
                    <tr><td>Track Number</td><td><input name="track_number" class="rm_ui_input_text" type="text" id="tagTrackNr" value="${track_number}" /></td></tr>
                </table>  
            </form>
        </div>
        <div class="rm_mbox_footer">
            <input type="button" class="rm_ui_def_button rm_mbox_btn_save" value="Save" />
            <input type="button" class="rm_ui_def_button rm_mbox_btn_close" value="Close" />
        </div>
    </div>
</div>
</script>
