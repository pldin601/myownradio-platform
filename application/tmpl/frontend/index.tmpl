<body class="{{ rootClass ? rootClass : '' }} {{ player.isLoaded ? 'player-visible' :'' }}" analytics-on="click"
      analytics-event="Click" analytics-category="Application">

<div class="busy-wrap table" ng-show="loader.busy === true">  <!-- todo: move to directive -->
    <div class="row">
        <div class="cell">
            <i class="icon-spinner9 rotate"></i><br>

            <div class="loading">WORKING...</div>
        </div>
    </div>
</div>

<now ng-if="player.isPlaying" ng-model="player.currentStream" on-track-change="player.nowPlaying"></now>

<div class="special">
    <div class="player-container" ng-if="player.isLoaded" mor-background-color="player.currentStream.cover_background">
        <div class="bg-gradient" ng-if="player.currentStream.cover_url">
            <div class="bg-image" style="background-image: url({{player.currentStream.cover_url}})"></div>
        </div>
        <div class="cool-player table">
            <div class="row">
                <div class="cell radio-cover">
                    <a ng-href="/streams/{{ player.currentStream.key }}">
                        <div class="cover">
                            <img ng-if="player.currentStream.cover" ng-src="{{ player.currentStream.cover_url }}?size=64">
                        </div>
                    </a>
                </div>
                <div class="cell radio-title">
                    <div class="title">
                        <a ng-href="/streams/{{ player.currentStream.key }}"
                           ng-bind="player.currentStream.name | uppercase"></a>
                    </div>
                </div>
                <div class="cell social-buttons">
                    <share ng-model="player.currentStream"></share>
                    <bookmark ng-model="player.currentStream"></bookmark>
                </div>
                <div class="cell track-title">
                <span ng-if="player.nowPlaying">
                    <span class="darker">ON AIR:</span> <span
                        ng-bind-template="{{ player.nowPlaying.artist | uppercase }} - {{ player.nowPlaying.title | uppercase }}"></span>
                </span>
                </div>
                <div class="cell right-tools">
                <span class="play-stop" ng-click="player.controls.switch()">
                    <i ng-class="player.isPlaying ? 'icon-stop2' : 'icon-play'" mor-tooltip="Play / Stop"></i>
                </span>
                    <span class="vertical-padding"></span>
                <span class="stop" ng-click="player.controls.unload()" mor-tooltip="Stop and close">
                    <i class="icon-switch"></i>
                </span>
                    <!--
                                    <span class="volume">
                                        <i class="icon-volume-up"></i>
                                    </span>
                    -->
                </div>
            </div>
        </div>
    </div>

    <header></header>
    <div id="contents">
        <div id="body">
            <div ng-view="" autoscroll=""></div>
        </div>
    </div>
    <footer></footer>
</div>
</body>
